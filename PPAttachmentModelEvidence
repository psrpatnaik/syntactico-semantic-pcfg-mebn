(CL:IN-PACKAGE "STELLA")

(DEFMODULE "/PL-KERNEL-KB/PL-USER/GENERATIVE_MODULE_1/FINDINGS_MODULE_1"
  :CASE-SENSITIVE? FALSE)

(IN-MODULE "/PL-KERNEL-KB/PL-USER/GENERATIVE_MODULE_1/FINDINGS_MODULE_1")

(IN-DIALECT :KIF)

(ASSERT (SUBJECT_LABEL MANSUBJ))
(ASSERT (SUBJECT_LABEL DOGSUBJ))
(ASSERT (SUBJECT_LABEL WOMANSUBJ))
(ASSERT (PREPOSITIONALPHRASEOBJECT_LABEL TELESCOPE))
(ASSERT (VERB_LABEL SAW))
(ASSERT (OBJECT_LABEL MANOBJ))
(ASSERT (OBJECT_LABEL WOMANOBJ))
(ASSERT (OBJECT_LABEL DOGOBJ))
(ASSERT (THING_LABEL DOMAIN_MFRAG.VP_MFRAG))
(ASSERT (PPOBJECTRELATESTOOBJECT MANOBJ TELESCOPE))
(ASSERT (PPOBJECTRELATESTOOBJECT WOMANOBJ TELESCOPE))
(ASSERT (not (PPOBJECTRELATESTOOBJECT DOGOBJ TELESCOPE)))
(ASSERT (not (PPOBJECTRELATESTOSUBJECTACT TELESCOPE DOGSUBJ)))
(ASSERT (PPOBJECTRELATESTOSUBJECTACT TELESCOPE MANSUBJ))
(ASSERT (PPOBJECTRELATESTOSUBJECTACT TELESCOPE WOMANSUBJ))
(ASSERT (PPOBJECTMODIFIESVERBACTION TELESCOPE SAW))
